{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.178",
    "gatsby-link": "^1.6.36",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "react-load-script": "^0.0.6",
    "styled-components": "^3.1.6"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "upload-s3": "s3-deploy './public/**' --cwd './public/' --region us-east-1 --bucket www.handlebarlabs.com --profile handlebarlabsS3",
    "deploy": "yarn run build && yarn run upload-s3"
  },
  "devDependencies": {
    "prettier": "^1.7.4",
    "s3-deploy": "^0.7.5"
  }
}
